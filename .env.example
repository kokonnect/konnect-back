# Copy this file to .env and fill in your actual values

# Database Configuration
DB_URL=your_db_url_here
DB_USERNAME=root
DB_PASSWORD=your_password

# Server Configuration
SERVER_PORT=8080

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Base URL (for OAuth redirect)
baseUrl=http://localhost:8080

# OAuth2 Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
KAKAO_CLIENT_ID=your_kakao_client_id_here
KAKAO_CLIENT_SECRET=your_kakao_client_secret_here

# JWT Configuration (optional - has default value)
JWT_REFRESH_TOKEN_VALIDITY=604800

# Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# School Info API (학교알리미)
SCHOOL_INFO_API_KEY=your_school_info_api_key_here